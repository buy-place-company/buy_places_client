<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorAccent">@color/accent</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ClusterItemBackgroundColor">
        <item name="android:background">#e3fbe734</item>
    </style>

    <style name="ClusterRenderBackgroundColor">
        <item name="android:background">#e3fb253a</item>
    </style>

    <style name="ActionBarStyle" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:background">?attr/colorPrimary</item>
    </style>

    <style name="ActionBarPopupTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:background">?attr/background</item>
    </style>
</resources>
